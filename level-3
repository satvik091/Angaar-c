1-
#include <stdio.h>

int main()
{
    int a,n,count=0;
    scanf("%d",&a);
    n=a;
    int i=1;
    while(i<=n){
        if(n%i==0){
            count=count+1;
        }
        i++;
    }
    if(count==2){
        printf("Yes Prime.");
    }else{
        printf("No Not a Prime.");
    }

    return 0;
}
2-
#include <stdio.h>

int main()
{
    int n;
    int cn;
    int i,j;
    int count;
    
    scanf("%d",&n);
    
    for(i=1;i<=n;i++){
        cn=i;
        count=0;
        
        for(j=1;j<=cn;j++){
            if(cn%j==0){
                count++;
            }
        }
        if(count==2){
            printf("%d\n",cn);
        }
    }
    return 0;
}
3-
#include <stdio.h>

int main()
{
    int a,n;
    int cn;
    int i,j;
    int count;
    int sum=0;
    
    scanf("%d",&a);
    scanf("%d",&n);
    for(i=a;i<=n;i++){
        cn=i;
        count=0;
        
        for(j=1;j<=cn;j++){
            if(cn%j==0){
                count++;
            }
        }
        if(count==2){
            sum=sum+cn;
        }
    }
    printf("%d",sum);
    return 0;
}

4-
#include <stdio.h>
#include<math.h>
int main()
{
    int a;
    scanf("%d",&a);
    
    int s;
    s=pow(a,0.5);
    
    printf("%d",s);
    return 0;
}

