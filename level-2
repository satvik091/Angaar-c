1-
#include <stdio.h>

int main()
{
    int a,fac=1,sum=0,i;
    scanf("%d",&a);
    for(i=1;i<=a;i++){
        if(a%i==0){
            sum=sum+i;
        }
    }
    for(i=1;i<=a;i++){
        fac=fac*i;
    }
    printf("%d\n%d",sum,fac);
    if(fac==sum){
        printf("Yes");
    }else{
        printf("No");
    }
}
2-
#include <stdio.h>

int main()
{
    int i,a,sum=0,fac=1;
    scanf("%d",&a);
    
    if(a%2==0){
        for(i=0;i<=a;i++){
            sum=sum+i;
        }
        printf("%d",sum);
    }else{
        for(i=1;i<=a;i++){
            fac=fac*i;
        }printf("%d",fac);
    }
    return 0;
}
3-
#include <stdio.h>

int main()
{
   int a,b=0;
   scanf("%d",&a);
   
   while(a!=0){
       b=b+1;
       a=a/10;
   }printf("%d",b);
    return 0;
}
4-
#include <stdio.h>

int main()
{
   int a,b=0;
   scanf("%d",&a);
   
   while(a!=0){
       b=(a%10)+b;
       a=a/10;
   }printf("%d",b);
    return 0;
}
5-
#include <stdio.h>

int main() {
    int a,rev;
    scanf("%d",&a);
    
    while(a!=0){
        rev=(a%10) +rev*10 ;
        a=a/10;
    }
    printf("%d",rev);

    return 0;
}
6-
#include <stdio.h>

int main() {
    int a,rev;
    scanf("%d",&a);
    int n=a;
    while(a!=0){
        rev=(a%10) +rev*10 ;
        a=a/10;
    }
    printf("%d\n",rev);
    
    if(n==rev){
        printf("Yes Palindrome");
        
    }else{
        printf("No Palindrome");
    }

    return 0;
}
