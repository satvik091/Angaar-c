1-
int main()
{
    int n,i,j,k,l;
    scanf("%d",&n);
    k=0;
    l=1;
    for(i=1;i<=n;i++){
        if(n%i==0){
            k=k+i;
        }
    }
    for(j=1;j<=n;j++){
        l=l*i;
    }
    
    if(k==l){
        printf("YES");
    }else{
        printf("NO");
    }
   

    return 0;
}

